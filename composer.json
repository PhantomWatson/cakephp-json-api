{
    "name": "phantomwatson/cakephp-json-api",
    "description": "json-api specification plugin for CakePHP - http://jsonapi.org/",
    "type": "cakephp-plugin",
    "keywords": [ "cakephp", "json", "jsonapi" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Watson"
        },
        {
            "name": "Jasper Smet",
            "homepage": "https://github.com/josbeir/cakephp-jsonapi/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/josbeir/cakephp-json-api/issues",
        "source": "https://github.com/josbeir/cakephp-json-api"
    },
    "require": {
        "php": ">=7.1",
        "cakephp/cakephp": "~3.0",
        "neomerx/json-api": "^2.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "2.*",
        "phpunit/phpunit": "6.0"
    },
    "autoload": {
        "psr-4": {
            "JsonApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonApi\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "TestApp\\": "tests/test_app/TestApp"
        }
    }
}
